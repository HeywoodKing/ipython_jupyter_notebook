    "import numpy as np\n",
    "\n",
    "# 31. 如何忽略所有的 numpy 警告(尽管不建议这么做)?\n",
    "# 提示: np.seterr, np.errstate\n",
    "defaults = np.seterr(all='ignore')\n",
    "\n",
    "_ = np.seterr(**defaults)\n",
    "\n",
    "z = np.ones(1)/0\n",
    "print(z)\n",
    "\n",
    "with np.errstate(divide='ignore'):\n",
    "    z = np.ones(1)/0\n",
    "    print(z)"
